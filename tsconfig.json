{
  "compilerOptions": {
    "lib": ["es2017", "dom"],
    "noEmit": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitOverride": true,
    "downlevelIteration": true
  }
}
