{
    "compilerOptions": {
        "lib": ["es2015", "dom"],
        "isolatedModules": true,
        "noImplicitAny": true,
        "strict": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "noImplicitOverride": true,
    }
}
