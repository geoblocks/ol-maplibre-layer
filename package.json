{
  "name": "@geoblocks/ol-maplibre-layer",
  "description": "Geoblocks to render a maplibre GL JS map as an OpenLayers layer.",
  "version": "1.0.3",
  "license": "BSD-3-Clause",
  "repository": "github:geoblocks/ol-maplibre-layer",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "/src",
    "/lib",
    "CHANGES.md"
  ],
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "targets": {
    "demo": {
      "source": "examples/demo.html",
      "context": "browser",
      "publicUrl": "./"
    }
  },
  "alias": {
    "buffer": false
  },
  "scripts": {
    "eslint": "eslint src examples",
    "start": "parcel serve examples/**/*.html",
    "build": "parcel build --target demo",
    "prepare": "tsc --pretty",
    "typecheck": "tsc --pretty --noEmit",
    "lint": "npm run eslint && npm run typecheck",
    "doc": "typedoc",
    "gh-pages": "rm -rf dist && npm run doc && npm run build && gh-pages -d dist"
  },
  "peerDependencies": {
    "maplibre-gl": ">=2.0.4",
    "ol": "^6 || ^7 || ^8 || ^9 || ^10"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.14.1",
    "@parcel/transformer-typescript-types": "2.14.1",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "eslint": "8.57.1",
    "gh-pages": "6.3.0",
    "maplibre-gl": "4.7.1",
    "ol": "10.4.0",
    "parcel": "2.14.1",
    "typedoc": "0.28.1",
    "typescript": "5.8.2"
  }
}
