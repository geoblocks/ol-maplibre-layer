{
  "name": "@geoblocks/ol-maplibre-layer",
  "version": "0.1.2",
  "license": "BSD-3-Clause",
  "repository": "github:geoblocks/ol-maplibre-layer",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "source": "src/ol-maplibre-layer.ts",
  "main": "dist/ol-maplibre-layer.js",
  "types": "dist/ol-maplibre-layer.d.ts",
  "files": [
    "/src",
    "/dist"
  ],
  "targets": {
    "demo": {
      "context": "browser",
      "distDir": "build",
      "publicUrl": "./"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "parcel build",
    "build-demo": "parcel build --target demo examples/*.html",
    "lint": "eslint src/* examples/*.js",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "parcel serve examples/*.html",
    "typecheck": "tsc",
    "gh-pages": "rm -rf build && npm run build-demo && gh-pages -d build"
  },
  "peerDependencies": {
    "maplibre-gl": ">=2.0.4",
    "ol": ">=6.10"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.10.3",
    "@parcel/transformer-typescript-types": "2.10.3",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "gh-pages": "6.1.0",
    "maplibre-gl": "3.6.2",
    "ol": "8.2.0",
    "parcel": "2.10.3",
    "prettier": "3.1.0",
    "typescript": "5.3.2"
  }
}
